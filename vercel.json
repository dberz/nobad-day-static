{
  "version": 2,
  "outputDirectory": "static-mirror",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/blogs/news",
      "destination": "/blogs/news.html"
    },
    {
      "source": "/products/(.*)",
      "destination": "/products/$1.html"
    },
    {
      "source": "/pages/(.*)",
      "destination": "/pages/$1.html"
    },
    {
      "source": "/collections/(.*)",
      "destination": "/collections/$1.html"
    },
    {
      "source": "/cart",
      "destination": "/cart.html"
    }
  ]
}
