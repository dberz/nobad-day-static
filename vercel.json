{
  "version": 2,
  "outputDirectory": "static-mirror",
  "rewrites": [
    {
      "source": "/blogs/news/:slug",
      "destination": "/blogs/news/:slug.html"
    },
    {
      "source": "/blogs/news",
      "destination": "/blogs/news.html"
    },
    {
      "source": "/products/:slug",
      "destination": "/products/:slug.html"
    },
    {
      "source": "/pages/:slug",
      "destination": "/pages/:slug.html"
    },
    {
      "source": "/collections/:slug",
      "destination": "/collections/:slug.html"
    },
    {
      "source": "/cart",
      "destination": "/cart.html"
    }
  ]
}
